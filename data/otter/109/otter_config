# DNA from SQLite
#   this is not actually loaded via the usual delayed column management,
#   but is used to construct the URL for the DNA stanza in AceDatabase->_zmap_dna_config()
#   which is loaded immediately when ZMap starts.

[default.filter.DNA]
classification=~ Otter > Assembly
internal=always_on
description=Genomic DNA
script_name=dna_get
resource_bin=local
priority=0

# Genomic_canonical from SQLite

[default.filter.Genomic_canonical]
classification=~ Otter > Assembly
internal=always_on
description=Genome assembly
script_name=tilepath_get
resource_bin=local
priority=1

# Curated features for SQLite

[default.filter.curated_features]
classification=~ Otter > Annotation
internal=always_on
description=Curated features
script_name=localdb_get
resource_bin=local
priority=1
featuresets=curated_features,polyA-seq_signal,polyA-seq_site,polyA_signal,polyA_site,pseudo_polyA,RSS,TATA_box,Ig_Feature,EUCOMM
analysis=polyA-seq_signal,polyA-seq_site,polyA_signal,polyA_site,pseudo_polyA,RSS,TATA_box,Ig_Feature,EUCOMM
feature_kind=SimpleFeature
# this fudge gets the analyses gff_source fields into the GFF
transcript_analyses=1
zmap_column=curated_features

# OTF via GFF and on_the_fly

[default.filter.OTF_Protein]
classification=~ Otter > On-the-fly > Genomic
internal=on_the_fly_genomic
description=OTF of protein
script_name=on_the_fly
analysis=OTF_Protein
feature_kind=ProteinSplicedAlignFeature
sequence_db=uniprot,uniprot_archive
blixem_data_type=protein-match
resource_bin=local

[default.filter.OTF_AdHoc_Protein]
classification=~ Otter > On-the-fly > Genomic
internal=on_the_fly_genomic
description=OTF of unknown protein
script_name=on_the_fly
analysis=OTF_AdHoc_Protein
feature_kind=ProteinSplicedAlignFeature
blixem_data_type=protein-match
resource_bin=local

