zmap_column=RNAseq_introns_canonical_minus
server_script=get_gff/features
analysis=ens_rnaseq_merged_canonical_minus
metakey=zfish_ens_rnaseq_10
description=merged RNAseq (WTSI), minus strand
feature_kind=SimpleFeature
zmap_style=rnaseq_introns_rev

[zebrafish.filter.all-merged_noncanon_plus_RNAseq_introns]
classification=RNA-seq > introns > EnsEMBL > Built on GRCz10 > non_canonical
zmap_column=RNAseq_introns_non_canonical_plus
server_script=get_gff/features
analysis=ens_rnaseq_merged_non_canonical_plus
metakey=zfish_ens_rnaseq_10
description=merged RNAseq (WTSI), plus strand
feature_kind=SimpleFeature
zmap_style=rnaseq_introns_non_canonical_fwd

[zebrafish.filter.all-merged_noncanon_minus_RNAseq_introns]
