classification=EnsEMBL > genes
server_script=get_gff/genes
metakey=ensembl_estgene_db_head
analysis=estgene
url_string=http://www.ensembl.org/Sus_scrofa/Location/View?gene=%(id)s
description=genes from Ensembl's EST clustering gene build system
content_type=transcript

[pig.filter.EST_Pig]
classification=EnsEMBL > pipeline
server_script=get_gff/features
analysis=EST2Genome_pig
description=Pig EST blast hits realigned using est2genome
feature_kind=DnaDnaAlignFeature
zmap_style=EST_align
swap_strands=1
content_type=alignment_feature
resource_bin=core_with_seq
blixem_data_type=dna-match
sequence_db=emblnew,emblrelease,refseq


