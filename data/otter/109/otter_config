# a new database of CCDS genes provided and supported by Felix:
[mouse.filter.CCDS_Gene]
classification=EnsEMBL > genes > CCDS
server_script=get_gff/genes
metakey=ens_livemirror_ccds_db
analysis=ccds_gene
url_string=http://www.ensembl.org/Mus_musculus/Location/View?gene=%(id)s
description=CCDS genes from latest ccds db
content_type=transcript

[cow.filter.ensembl_EST_genes]
